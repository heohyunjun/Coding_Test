      "cell_type": "code",
      "source": [
        "def solution(n):\n",
        "    num = ['1','2','4']\n",
        "    answer = \"\"\n",
        "\n",
        "    while n > 0:\n",
        "        n -= 1\n",
        "        answer = num[n % 3] + answer\n",
        "        n //= 3\n",
        "\n",
        "    return answer"
      ],
